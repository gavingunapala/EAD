            android:name=".StationOwnerProfile"
            android:exported="true" />
        <activity
            android:name=".StationOwnerRegister"
            android:exported="true" />
        <activity
            android:name=".VehicleOwnerRegister"
            android:exported="true" />
        <activity
            android:name=".VehicleOwnerProfile"
            android:exported="true" />
        <activity
            android:name=".Login"
            android:exported="true" />
        <activity
            android:name=".common.MainActivity"
            android:exported="true" />
    </application>

</manifest>